{
  "presets": [
    [
      "@babel/preset-env",
      {
        "corejs": "3.6.4",
        "useBuiltIns": "usage",
        "targets": {
          "ie": 8
        }
      }
    ],
    [
      "@babel/preset-react",
      {
        "pragma": "teddy.h", // default pragma is React.createElement
        "pragmaFrag": "", // default is React.Fragment
        "throwIfNamespace": false // defaults to true
      }
    ]
  ]
}
